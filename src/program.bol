class Math
  method factorial(n)
  vars f, temp, ret, one
  begin
  one = 1
  if n eq one then
    return n
  else
    temp = n - one
    f = self.factorial(temp)
    ret = f * n
    return ret
  end-if

  end-method

end-class

class Bhaskara
  vars a, b, c, delta

  method getDelta()
  vars bSquare, aTimesC, negFour, temp
  begin
    bSquare = self.b * self.b
    aTimesC = self.a * self*c
    negFour = -4
    temp = self.delta
    self.delta = negFour * aTimesC
    self.delta = bSquare + temp
    temp = self.delta
    return temp
  end-method

end-class


